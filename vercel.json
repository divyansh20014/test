{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.12" }
        }
    ],
    "routes": [
        {
            "src": "/api/combined-data",
            "dest": "/api/combined-data"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
